generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Post {
  id    String  @id @default(uuid())
  title String
  img   String?
}

model User {
  id    String  @id @default(uuid())
  username String
  email    String @unique
  password String
}
